<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserRegisterActivity">

    <EditText
        android:id="@+id/email"
        android:layout_width="194dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="text"
        android:text="Email"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="214dp"
        tools:ignore="Autofill,HardcodedText,LabelFor,MissingConstraints,TextFields" />

    <ImageView
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:srcCompat="@drawable/email"
        tools:layout_editor_absoluteX="78dp"
        tools:layout_editor_absoluteY="224dp"
        tools:ignore="ContentDescription,MissingConstraints" />

    <EditText
        android:id="@+id/password"
        android:layout_width="194dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textPassword"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="108dp"
        tools:layout_editor_absoluteY="273dp"
        tools:ignore="Autofill,LabelFor,MissingConstraints" />

    <ImageView
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:srcCompat="@drawable/lock_password"
        tools:layout_editor_absoluteX="78dp"
        tools:layout_editor_absoluteY="285dp"
        tools:ignore="ContentDescription,DuplicateIds,MissingConstraints" />

    <EditText
        android:id="@+id/reEnterPass"
        android:layout_width="194dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textPassword"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="107dp"
        tools:layout_editor_absoluteY="328dp"
        tools:ignore="Autofill,LabelFor,MissingConstraints" />

    <ImageView
        android:layout_width="20dp"
        android:layout_height="20dp"
        app:srcCompat="@drawable/lock_password"
        tools:layout_editor_absoluteX="78dp"
        tools:layout_editor_absoluteY="341dp"
        tools:ignore="ContentDescription,MissingConstraints" />

    <CheckBox
        android:id="@+id/checkBoxAgree"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:text="I agree to the Term of Use"
        android:textSize="12sp"
        tools:layout_editor_absoluteX="108dp"
        tools:layout_editor_absoluteY="388dp"
        tools:ignore="HardcodedText,MissingConstraints" />

    <Button
        android:id="@+id/buttonRegister"
        android:layout_width="270dp"
        android:layout_height="50dp"
        android:text="REGISTER"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="460dp"
        tools:ignore="HardcodedText,MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>